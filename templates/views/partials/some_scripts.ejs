<script>
    // to show the underline in the liste of categories test
    const activeurl = window.location.pathname;

    document.querySelectorAll(".categories a")
        .forEach((e) => {
            if (e.href.includes(activeurl)) {
                e.classList.add("active");
            }
            // console.log(e.href)
        });


</script>